@ARTICLE{Byock2005195,
author={Byock, J. and Walker, P. and Erlandson, J. and Holck, P. and Zori, D. and Gudmundsson, M. and Tveskov, M.},
title={A Viking-age valley in Iceland: The Mosfell Archaeological Project},
journal={Medieval Archaeology},
year={2005},
volume={49},
pages={195-218},
doi={10.1179/007660905x54080},
note={cited By 21},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-38949105868&doi=10.1179%2f007660905x54080&partnerID=40&md5=f689883ff7e2e35e8bb70a609248da35},
abstract={THIS is an account of both the history and the recent findings of the Mosfell Archaeological Project. Excavation is part of an interdisciplinary research approach that uses archaeology, history, anthropology, forensics, environmental sciences and saga studies to construct a picture of human habitation, power relationships, religious and mortuary practices, and environmental change in the region of Mosfellssveit in south-western Iceland. The valley system with surrounding highlands and lowland coastal areas has interlocking natural and cultural components which developed from the 9th-century settlement of Iceland into a Viking Age chieftaincy dominated by the family at Mosfell/Hrísbrú. Excavations of both pagan and Christian sites are providing significant information on the changing periods of occupation, with implications for the larger study of Viking North Atlantic. During the Viking Age, Mosfell was a self-contained social and economic unit connected to the rest of Iceland through a network of roads, including a major E.-W. route to the nearby assembly place for the yearly Althing. With its ship's landing or port at Leirvogur, in the bay at the valley's mouth, the region was in commercial and cultural contact with the larger Scandinavian and European worlds.},
references={Nordal, S., Jónsson, G., Gunnlaugs saga ormstungu (1938) Borgfiroinga Sögur, , Íslenzk fornrit, 3, Reykjavík, ch. 13; We also work in collaboration with the Hólar Project directed by Ragnheiour Traustadóttir; 'Nafnaskrá íslenzkra presta', in Diplomatarium Islandicum: Íslenzkt fornbréfasafn, ed; Jón Sigurosson (Copenhagen, 1857) I, 186. This list of priests is widely believed to have been written by Ari Thorgilsson, the author of Íslendingabók. Skapti is also mentioned in The Saga of Thorgils and Haflioi Porgils saga ok Haflioa, ch. 31, in Sturlunga saga I, eds; Jón Jóhannesson, Magnús Finnbogason and Kristján Eldjárn [Reykjavík, 1946]); Nordal, S., (1933) Egils Saga Skalla-Grímssonar, , Íslenzk fornrit, 2, Reykjavík, lvii; Haroarson, P., Sjúkdómur Egils Skallagrímssonar (1984) Skírnir, 158, pp. 245-248; Byock, J., The skull and bones in Egils Saga: A Viking, a grave, and Paget's Disease (1993) Viator, 24, pp. 23-50; Hauskúpan og beinin í Egils sögu (1994) Skírnir, pp. 73-109; Egil's Bones: A Viking Warrior and Paget's Disease (1995) Scientific American, 272 (1), pp. 82-87; Ól. Sveinsson, E., Hallfreoar saga (1933) Vatnsdæla Saga, , Íslenzk fornrit, 8, Reykjavik, ch. 11; Simpson, I.A., Vésteinsson, O., Adderley, W.P., McGovern, T.H., Fuel resource utilisation in landscapes of settlement (2003) Archaeol. Science, 30, pp. 1401-1420; Thorarinsson, S., Pjósárdalur under landnamstid och sagatid (1943) Forntida Gårdar I Island, 17-21. , M. Stenberger ed, Copenhagen; Thanks are owed to Bjarki Bjarnason, who, in the initial survey phase of the project and afterwards, walked the valley, coast, and highlands with Jesse Byock, sharing his extraordinary knowledge of Mosfellssveit's past. Professor Helgi Porláksson also worked closely with us on textual and archival matters; Helgason, A., Siguroardóttir, S., Gulcher, J.R., Ward, R., Stéfansson, K., MtDNA and the origin of the Icelanders: Deciphering signals of recent population history (2000) American J. Human Genetic