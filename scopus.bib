@ARTICLE{Hartman2017123,
author={Hartman, S. and Ogilvie, A.E.J. and Ingimundarson, J.H. and Dugmore, A.J. and Hambrecht, G. and McGovern, T.H.},
title={Medieval Iceland, Greenland, and the New Human Condition: A case study in integrated environmental humanities},
