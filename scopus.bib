@ARTICLE{Bolender201865,
author={Bolender, D.J. and Johnson, E.D.},
title={Reassembling the household for Icelandic archaeology: a contribution to comparative political economy [Réassembler le foyer pour l'archéologie d'Islande : contribution à l'économie politique comparative] [Wiederaufbau des haushalts für isländische archäologie: ein beitrag zur vergleichenden politischen ökonomie] [Fare ordine nell'archeologia domestica islandese: un contributo all'economica politica comparativa] [Volviendo a montar el hogar para la arqueología islandesa: una contribución a la economía política comparada]},
